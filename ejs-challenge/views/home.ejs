<%- include("partials/header") %>

<h1>Home</h1>
<p><%- startingContent %></p> 

<% newPost.forEach(function(post) { %>
    <h1><%= post.title %></h1>
    <% var length = 100 %>
    <p>
        <%= post.content.substring(0, length) + " ..." %>
        <a href="/posts/<%= post.title %>">Read More</a>
    </p>
<% }) %>

<%- include("partials/footer") %>